
#include "../libft.h"
#include <stdio.h>
#include <ctype.h>

int	main(void)
{
	int	n;

	write(1, "\n------------------ FT_ISALPHA ------------------\n\n", 51);
	n = ft_isalpha('A');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('A');
	printf("isalpha:    %d\n", n);

	n = ft_isalpha('Z');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('Z');
	printf("isalpha:    %d\n", n);


	n = ft_isalpha('a');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('a');
	printf("isalpha:    %d\n", n);

	n = ft_isalpha('z');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('z');
	printf("isalpha:    %d\n", n);

	n = ft_isalpha('*');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('*');
	printf("isalpha:    %d\n", n);

	n = ft_isalpha('/');
	printf("ft_isalpha: %d\n", n);
	n = isalpha('/');
	printf("isalpha:    %d\n", n);

	return (0);
}
